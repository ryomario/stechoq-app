<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".RegisterActivity">
    <TextView
            android:text="REGISTER"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView9"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:textSize="24sp" android:textStyle="bold" android:fontFamily="@font/poppins"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.060000002"/>
    <Button
            android:text="Register"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/registerButton"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword2"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.120000005"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:ems="10"
            android:id="@+id/editTextTextPassword2"
            app:layout_constraintStart_toStartOf="@+id/editTextTextPassword3"
            app:layout_constraintEnd_toEndOf="@+id/editTextTextPassword3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword3"
            app:layout_constraintVertical_bias="0.04000002"/>
    <TextView
            android:text="Confirm Password"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView4"
            android:textColor="#000000" app:layout_constraintStart_toStartOf="@+id/editTextTextPassword2"
            app:layout_constraintBottom_toTopOf="@+id/editTextTextPassword2"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:ems="10"
            android:id="@+id/editTextTextPassword3"
            app:layout_constraintStart_toStartOf="@+id/editTextTextEmailAddress3"
            app:layout_constraintEnd_toEndOf="@+id/editTextTextEmailAddress3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress3"
            app:layout_constraintVertical_bias="0.04000002"/>
    <TextView
            android:text="Password"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView5"
            android:textColor="#000000" app:layout_constraintStart_toStartOf="@+id/editTextTextPassword3"
            app:layout_constraintBottom_toTopOf="@+id/editTextTextPassword3"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textEmailAddress"
            android:ems="10"
            android:id="@+id/editTextTextEmailAddress2"
            app:layout_constraintStart_toStartOf="@+id/editTextTextEmailAddress5"
            app:layout_constraintEnd_toEndOf="@+id/editTextTextEmailAddress5"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress5"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.04000002"/>
    <TextView
            android:text="Email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView6"
            android:textColor="#000000" app:layout_constraintStart_toStartOf="@+id/editTextTextEmailAddress2"
            app:layout_constraintBottom_toTopOf="@+id/editTextTextEmailAddress2"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textEmailAddress"
            android:ems="10"
            android:id="@+id/editTextTextEmailAddress3"
            app:layout_constraintStart_toStartOf="@+id/editTextTextEmailAddress2"
            app:layout_constraintEnd_toEndOf="@+id/editTextTextEmailAddress2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress2"
            app:layout_constraintVertical_bias="0.04000002"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textEmailAddress"
            android:ems="10"
            android:id="@+id/editTextTextEmailAddress5"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView9" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.089999974"/>
    <TextView
            android:text="Username"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView7"
            android:textColor="#000000" app:layout_constraintStart_toStartOf="@+id/editTextTextEmailAddress3"
            app:layout_constraintBottom_toTopOf="@+id/editTextTextEmailAddress3"/>
    <TextView
            android:text="Full Name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView8"
            android:textColor="#000000" app:layout_constraintStart_toStartOf="@+id/editTextTextEmailAddress5"
            app:layout_constraintBottom_toTopOf="@+id/editTextTextEmailAddress5"/>
</androidx.constraintlayout.widget.ConstraintLayout>